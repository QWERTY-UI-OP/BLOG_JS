<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <link rel="stylesheet" type="text/css" href="/styles.css">
</head>
<body>
  <header>
    <h1>Blog</h1>
    <a href="/new">Create New Post</a>
  </header>
  <div class="container">
    <h1>Blog Posts</h1>
    <ul>
      <% posts.forEach(post => { %>
        <li>
          <a href="/post/<%= post.id %>"><%= post.title %></a>
        </li>
      <% }) %>
    </ul>
  </div>
</body>
</html>